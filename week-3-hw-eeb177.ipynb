   "source": [
    "#Calculating AT content\n",
    "dna = \"ACTGATCGATTACGTATAGTATTTGCTATCATACATATATATCGATGCGTTCAT\"\n",
    "length = len(dna)\n",
    "a_count = dna.count(\"A\")\n",
    "t_count = dna.count(\"T\")\n",
    "at_content = (a_count + t_count)/length\n",
    "print (\"AT content is \"+str(at_content))\n"
   ]
